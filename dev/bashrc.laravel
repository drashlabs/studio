# php - pass the version number to switch to (5/7)
alias php.v="sh bash/dev/setup.php.versions.sh "

# laravel-artisan
alias a.mf="php artisan migrate:fresh"
alias a.mfs="php artisan migrate:fresh --seed"
alias a.serve="php artisan serve"

# laravel-mix
alias mix.compile="npm run dev"
alias mix.watch="npm run watch"